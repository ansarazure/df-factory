{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dattafactory78686"
		},
		"ADLS2CCSVcust_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS2CCSVcust'"
		},
		"ADLSParquet_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSParquet'"
		},
		"ADLScsv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLScsv'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSynapseAnalytics2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics2'"
		},
		"LinkSVCMetadata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LinkSVCMetadata'"
		},
		"PoolDBDimCust_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PoolDBDimCust'"
		},
		"ADLS2CCSVcust_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2storeacct12343.dfs.core.windows.net/"
		},
		"ADLSParquet_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2storeacct12343.dfs.core.windows.net/"
		},
		"ADLScsv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2storeacct12343.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2storeacct12343.dfs.core.windows.net/"
		},
		"LinkSVCMetadata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2storeacct12343.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dattafactory78686_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS2CCSVcust_accountKey": {
						"value": "[parameters('ADLS2CCSVcust_accountKey')]"
					},
					"ADLSParquet_accountKey": {
						"value": "[parameters('ADLSParquet_accountKey')]"
					},
					"ADLScsv_accountKey": {
						"value": "[parameters('ADLScsv_accountKey')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSynapseAnalytics2_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics2_connectionString')]"
					},
					"LinkSVCMetadata_accountKey": {
						"value": "[parameters('LinkSVCMetadata_accountKey')]"
					},
					"PoolDBDimCust_connectionString": {
						"value": "[parameters('PoolDBDimCust_connectionString')]"
					},
					"ADLS2CCSVcust_properties_typeProperties_url": {
						"value": "[parameters('ADLS2CCSVcust_properties_typeProperties_url')]"
					},
					"ADLSParquet_properties_typeProperties_url": {
						"value": "[parameters('ADLSParquet_properties_typeProperties_url')]"
					},
					"ADLScsv_properties_typeProperties_url": {
						"value": "[parameters('ADLScsv_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"LinkSVCMetadata_properties_typeProperties_url": {
						"value": "[parameters('LinkSVCMetadata_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}