{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dattafactory78686"
		},
		"ADLSParquet_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSParquet'"
		},
		"ADLScsv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLScsv'"
		},
		"ADLSParquet_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2storeacct12343.dfs.core.windows.net/"
		},
		"ADLScsv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2storeacct12343.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dattafactory78686_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSParquet_accountKey": {
						"value": "[parameters('ADLSParquet_accountKey')]"
					},
					"ADLScsv_accountKey": {
						"value": "[parameters('ADLScsv_accountKey')]"
					},
					"ADLSParquet_properties_typeProperties_url": {
						"value": "[parameters('ADLSParquet_properties_typeProperties_url')]"
					},
					"ADLScsv_properties_typeProperties_url": {
						"value": "[parameters('ADLScsv_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}